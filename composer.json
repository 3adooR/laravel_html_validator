  "version": "1.0.0",
  "type": "library",
  "autoload": {
    "psr-4": {
      "IDM\\LaravelHtmlValidator\\": "src/"
    }
  },
  "require": {
    "illuminate/support": "^8.40",
    "ext-curl": "*",
    "php": ">=7.2"
